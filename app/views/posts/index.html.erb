<h1>Listado de Posts:</h1>
<p><td><%= link_to "Crear Post", new_post_path%></p>
<table class="table">
  <tr>
    <th>Id</th>
    <th>Title</th>
    <th>Usuario</th>
    <th>Acciones</th>
    <th>Acciones</th>
    <th>Eliminar</th>
  </tr>
<%@posts.each do |post|%>
  <tr>
    <td><%= post.id%></td>
    <td><%= post.title%></td>
    <td><%= post.user.email if post.user%></td>
    <td><%= link_to "Ver",post%></td>
    <td><%= link_to "Editar", edit_post_path(post)%></td>
    <td><%= link_to "Eliminar", post_path(post), method: :delete, data: {confirm: "Seguro?"}%></td>
  </tr>
<%end%>
</table>
